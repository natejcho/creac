{
  "name": "create-react-component",
  "version": "0.0.1",
  "description": "Script to create a new React component",
  "main": "./lib/index",
  "readme": "README.md",
  "engines": {
    "npm": ">=6.12.1"
  },
  "scripts": {
    "dev": "node ./lib/index",
    "lint": "eslint lib bin",
    "lint:fix": "eslint --fix lib bin"
  },
  "keywords": [
    "cli",
    "scaffold",
    "react",
    "create-react-app",
    "create-react-component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natejcho/create-react-component.git"
  },
  "author": "Nathaniel Cho",
  "license": "MIT",
  "devDependencies": {
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.7",
    "lint-staged": "^10.0.0-beta.15",
    "prettier": "^1.19.1"
  },
  "bin": {
    "crcf": "./bin/crcf.js"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.17.1",
    "cosmiconfig": "^5.1.0",
    "fs": "0.0.1-security",
    "log-update": "^3.1.0",
    "mkdirp": "^0.5.1",
    "node-alias": "^1.0.4",
    "semver": "^5.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
